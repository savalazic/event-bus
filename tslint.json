{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-config-airbnb"
  ],
  "rules": {
    "variable-name": false,
    "import-name": false,
    "ordered-imports": false,
    "interface-name": false,
    "object-literal-sort-keys": false,
    "ter-arrow-parens": false
  }
}
